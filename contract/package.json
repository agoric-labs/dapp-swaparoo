  },
  "devDependencies": {
    "@endo/bundle-source": "^2.7.0",
    "@endo/eslint-plugin": "^0.5.1",
    "@endo/init": "^0.5.59",
    "@endo/ses-ava": "^0.2.43",
    "@agoric/deploy-script-support": "^0.10.3",
    "@jessie.js/eslint-plugin": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
